{
	"name": "util.log",
	"version": "0.0.35",
	"author": "jmquigley <jmquigley@outlook.com>",
	"description": "A simple logging utility module",
	"main": "./index.js",
	"types": "./index.d.ts",
	"private": false,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/jmquigley/util.log"
	},
	"bugs": {
		"url": "https://github.com/jmquigley/util.log/issues"
	},
	"engines": {
		"node": ">=7.x"
	},
	"dependencies": {
		"chalk": "^2.4.2",
		"fs-extra": "^7.0.1",
		"sprintf-js": "https://github.com/jmquigley/sprintf.js.git#dc62b1c",
		"util.join": "0.0.21",
		"util.timestamp": "0.0.38",
		"uuid": "^3.3.2"
	},
	"devDependencies": {
		"@babel/cli": "^7.2.3",
		"@babel/core": "^7.3.3",
		"@babel/preset-env": "^7.3.1",
		"@babel/preset-typescript": "^7.3.3",
		"@babel/runtime": "^7.3.1",
		"@types/chalk": "^2.2.0",
		"@types/fs-extra": "5.0.5",
		"@types/node": "^11.9.4",
		"@types/sprintf-js": "1.1.2",
		"@types/uuid": "^3.4.4",
		"coveralls": "^3.0.2",
		"jest": "^24.1.0",
		"prettier": "^1.16.4",
		"tslint": "^5.12.1",
		"tslint-config-prettier": "^1.18.0",
		"typescript": "^3.3.3",
		"util.fixture": "0.0.65",
		"util.pkgwrap": "^0.0.94"
	},
	"pkgwrap": {
		"cleanup": [
			"index.d.ts",
			"index.js"
		]
	},
	"jest": {
		"collectCoverage": true,
		"coveragePathIgnorePatterns": [
			"<rootDir>/__tests__"
		],
		"moduleFileExtensions": [
			"js",
			"jsx",
			"ts",
			"tsx",
			"json"
		],
		"notify": false,
		"testPathIgnorePatterns": [
			"__tests__/helpers"
		],
		"verbose": false
	},
	"scripts": {
		"all": "yarn install && yarn run prettier && yarn run build && yarn test",
		"build": "yarn clean && yarn run lint && pkgwrap --build",
		"clean": "pkgwrap --clean",
		"coverage": "pkgwrap --coverage --jest",
		"lint": "pkgwrap --lint",
		"prepublishOnly": "yarn run all",
		"prettier": "prettier --write \"**/*.{ts,tsx,json,yaml,yml,css}\"",
		"test": "pkgwrap --testing --jest"
	}
}
